proc disable_all {path} {
    catch {$path configure -state disabled}
    foreach child [winfo children $path] {
        disable_all $child
    }
}

proc enable_all {path} {
    catch {$path configure -state normal}
    foreach child [winfo children $path] {
        enable_all $child
    }
}

